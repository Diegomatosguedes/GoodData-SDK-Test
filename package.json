{
  "build": {
    "appId": "TOTVS.Agent.Analytics",
    "productName": "TOTVS Agent Analytics",
    "files": [
      "dist/TOTVS-Agent-Analytics/*",
      "dist/TOTVS-Agent-Analytics/src-angular/**/*",
      "dist/TOTVS-Agent-Analytics/src-electron/**/*",
      "dist/TOTVS-Agent-Analytics/icons/**/*",
      "dist/TOTVS-Agent-Analytics/i18n/**/*",
      "!dist/TOTVS-Agent-Analytics-Server/remote/assets/**/*",
      "!dist/TOTVS-Agent-Analytics-Server/remote/logs/**/*",
      "!dist/TOTVS-Agent-Analytics/assets/**/*",
      "!dist/TOTVS-Agent-Analytics/src-electron/encryption/keys/agent.rsa.public",
      "!dist/TOTVS-Agent-Analytics/src-electron/encryption/keys/agent.rsa.private"
    ],
    "extraFiles": [
      "java",
      "icons"
    ],
    "directories": {
      "output": "release-builds",
      "buildResources": "dist"
    },
    "asar": true,
    "asarUnpack": [
      "assets/*",
      "logs/*"
    ],
    "win": {
      "icon": "icons/windows/analytics.ico",
      "target": {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      },
      "publish": [
        "github"
      ]
    },
    "linux": {
      "icon": "icons/linux/analytics.png",
      "target": [
        "deb"
      ],
      "publish": [
        "github"
      ]
    },
    "mac": {
      "icon": "icons/linux/analytics.png",
      "target": [
        "dmg"
      ],
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "warningsAsErrors": false,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "oneClick": false,
      "perMachine": true,
      "include": "./installers/installer.nsh"
    }
  },
  "name": "totvs-carol-analytics-frontend",
  "version": "1.0.0",
  "author": "Diego Matos Guedes <suporte.gd@totvs.com.br> (https://www.totvs.com/)",
  "homepage": "https://totvstfs.visualstudio.com/TOTVS-GoodData/_git/TOTVS-Carol-Analytics-FrontEnd",
  "description": "Interface gráfica da aplicação Carol-Analytics, da plataforma TOTVSApps.",
  "main": "./dist/TOTVS-Carol-Analytics-FrontEnd/site/index.html",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/TOTVS-GoodData/TOTVS-Carol-Analytics.git"
  },
  "scripts": {
    "start": "ng serve --ssl",
    "angular": "ng serve --ssl",
    "docker": "docker build --platform linux/amd64 -t totvs-carol-analytics-frontend:dev .",
    "build-angular": "ng build --configuration production",
    "azure": "git add . && git commit -m \"Build %date% %time%\" && git push"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "18.2.0",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "18.2.0",
    "@angular/core": "18.2.0",
    "@angular/forms": "18.2.0",
    "@angular/material": "^18.0.6",
    "@angular/platform-browser": "18.2.0",
    "@angular/platform-browser-dynamic": "18.2.0",
    "@angular/platform-server": "18.2.0",
    "@angular/router": "18.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@po-ui/ng-components": "18.2.0",
    "@po-ui/ng-sync": "18.2.0",
    "@progress/kendo-theme-default": "^6.3.0",
    "@totvs-apps-components/auth": "^18.0.0",
    "@totvs-apps-components/header": "^18.0.0",
    "@totvs/po-theme": "^15.15.0",
    "@totvs/thf-theme-kendo": "^4.0.1",
    "core-js": "^3.0.0",
    "path": "^0.12.7",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/compiler-cli": "18.2.0",
    "@angular/language-service": "18.2.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.0.0",
    "json-server": "^0.17.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.5"
  }
}